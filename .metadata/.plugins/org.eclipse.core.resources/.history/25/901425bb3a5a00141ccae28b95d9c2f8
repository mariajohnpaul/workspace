#include <stdio.h>
#include <stdlib.h>
int arr[100];

quicksort(int *arr,int st,int end)
{
	if((end-st)<=1) return;
	int Ost=st,Oend=end;
	int pv= arr[(st+end)/2];
	while(end>st)
	{
		while(arr[st]<pv)st++;
		while(arr[end]>pv)end--;
		if(end>st)
		{
			int temp=arr[st];
			arr[st]=arr[end];
			arr[end]=arr[st];
		}
	}
	Quicksort(arr,Ost,end);
	Quicksort(arr,st,Oend);
}

int main()
{

	for(int i=0;i<100;i++)
		{
		arr[i]=rand()%100;
		}
	quicksort(arr,0,100);

return 0;
}
