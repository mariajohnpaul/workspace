#include <stdio.h>
#include <stdlib.h>
int arr[100]={};

void quicksort(int *arr,int st,int end)
{
	if((end-st)<1) return;
	int Ost=st,Oend=end;
	int pv= arr[(st+end)/2];
	while(end>st)
	{
		while(arr[st]<pv)st++;
		while(arr[end]>pv)end--;
		if(end>st)
		{
			int temp=arr[st];
			arr[st]=arr[end];
			arr[end]=temp;
			st++;
			end--;
		}
	}
	quicksort(arr,Ost,end);
	quicksort(arr,end+1,Oend);
}

int main()
{
int i=0;
	for( i=0;i<100;i++)
		{
		arr[i]=rand()%100;
		printf("%d,",arr[i]);
		}
	arr[2]=100;
	quicksort(arr,0,99);
printf("\n");
	for(i=0;i<100;i++)
	{
		printf("%d,",arr[i]);
	}

return 0;
}
