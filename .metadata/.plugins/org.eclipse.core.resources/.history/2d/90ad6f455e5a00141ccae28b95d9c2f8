#include<iostream>
#include<algorithm>
using namespace std;
class MaxHeap
{
private :
	int arr[];
	int length;
public:
	MaxHeap(void)
{
		arr= new arr[100];
		length=0;
for(int i=0;i<100;i++)
{
	arr[i]=0;
}
}

void createHeap(int a[],int N)
{
for(int i=0;i<N;i++)
{
	addheap(a[i]);
}
}
void addheap(int el)
{
arr[length]=el;
}

};
void main()
{

	class MaxHeap *Hptr = new (MaxHeap);
    static int inp[100];
	for( int i=0;i<100;i++)
			{
			inp[i]=rand();
			}

}

